#=======================================================================
# Makefrag for rv32uzfh tests
#-----------------------------------------------------------------------

rv32uzfh_sc_tests = \
	fadd fdiv fclass fcmp fcvt fcvt_w fmadd fmin \
	ldst move recoding \

rv32uzfh_p_tests = $(addprefix rv32uzfh-p-, $(rv32uzfh_sc_tests))
rv32uzfh_v_tests = $(addprefix rv32uzfh-v-, $(rv32uzfh_sc_tests))
